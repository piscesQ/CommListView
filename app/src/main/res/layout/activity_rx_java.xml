<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.app.koreq.commlistview.activity.RxJavaActivity"
    android:orientation="vertical"
    >
    <LinearLayout
        android:id="@+id/rx_ll_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <Button
            android:id="@+id/rx_demo_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="rx_demo_1"
            />
        <Button
            android:id="@+id/rx_http_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="rx_http_1"
            />
    </LinearLayout>

    <TextView
        android:id="@+id/rx_text"
        android:layout_width="368dp"
        android:layout_height="453dp"
        android:background="@color/blue_00eeee"
        android:text="Show Result"
        android:layout_marginTop="8dp"
        />

</LinearLayout>
