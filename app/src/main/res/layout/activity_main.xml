<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.app.koreq.commlistview.activity.MainActivity"
    >

    <Button
        android:id="@+id/btn_comm_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CommList"
        />

    <Button
        android:id="@+id/btn_http_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/btn_comm_list"
        android:text="Http Test"
        />

    <Button
        android:id="@+id/btn_pairview_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/btn_http_test"
        android:text="Pair View Test"
        />
 <Button
        android:id="@+id/btn_rxjava_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/btn_pairview_test"
        android:text="Rxjava Test"
        />
 <Button
        android:id="@+id/btn_gradle_str_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/btn_rxjava_test"
        android:text="Gradle Res Str"
        />

</android.support.constraint.ConstraintLayout>
